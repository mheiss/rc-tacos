            <property name="dataSource">TacosDataSource</property>
            <method name="open"><![CDATA[// tmp var for looping during the fetch script
count = 0;

// the list of report parameters
reportParam = params["reportParam"];]]></method>
            <method name="fetch"><![CDATA[// loop and fill the result list
if(count < reportParam.size()){
	entry = reportParam.get(count);
	
	// custom logic goes here
	count++;
	dataSetRow["index"] = count;
	dataSetRow["name"] = entry.getSystemUser().getFirstName() + " "+ entry.getSystemUser().getLastName();
	dataSetRow["location"] = entry.getLocation().getDisplayString();
	dataSetRow["startTime"] = entry.getPlannedStartTime();
	dataSetRow["startDate"] = entry.getPlannedStartDate();
	dataSetRow["startDateTime"] = entry.getPlannedStartDateTime();
	dataSetRow["endTime"] = entry.getPlannedEndTime();
	dataSetRow["endDate"] = entry.getPlannedEndDate();
	dataSetRow["endDateTime"] = entry.getPlannedEndDateTime();
	dataSetRow["assignment"] = entry.getAssignment().getDisplayString();
	dataSetRow["serviceType"] = entry.getServiceType().getDisplayString();
	if(entry.getCar() != null)  {
		dataSetRow["car"] = entry.getCar().getDisplayString();
	} 
	
	return true;
}
// no more records
return false;]]></method>
            <method name="close"><![CDATA[// be fair and cleanup
reportParam=null;]]></method>
