            <property name="dataSource">rosterSource</property>
            <method name="open"><![CDATA[// tmp var for looping during the fetch script
count = 0;

// the list of roster entries is passed in as parameter
rosterList = params["rosterList"];


]]></method>
            <method name="fetch"><![CDATA[// loop and fill the result list
if(count < rosterList.size()){
	entry = rosterList.get(count);
	row["firstName"] = entry.getSystemUser().getFirstName();
	row["lastName"] = entry.getSystemUser().getLastName();
	row["plannedStart"] = entry.getPlannedStart();
	row["plannedEnd"] = entry.getPlannedEnd();
	row["realStart"] = entry.getRealStart();
	row["realEnd"] = entry.getRealEnd();
	row["assignment"] = entry.getAssignment().getId();
	row["serviceType"] = entry.getServiceType().getName();
	count++;
	return true;
}
// no more records
return false;
]]></method>
            <method name="close"><![CDATA[// be fair and cleanup
rosterList=null;]]></method>
