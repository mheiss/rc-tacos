            icon="icons/toolbar/viewTransportPrebooking.png"
            id="at.rc.tacos.client.view.prebooking_view">
      </view>     
      <view
            name="Dialyse"
            allowMultiple="false"
            class="at.rc.tacos.client.ui.view.DialysisView"
            icon="icons/toolbar/viewTransportDialyse.png"
            id="at.rc.tacos.client.view.dialysis_view">
      </view>
      <view
            allowMultiple="false"
            class="at.rc.tacos.client.ui.admin.view.ServiceTypeAdminView"
            id="at.rc.tacos.client.view.admin.serviceTypeAdminView"
            icon="icons/resources/serviceType.png"
            name="Dienstverhältnisse">
      </view>
      <view
            allowMultiple="false"
            class="at.rc.tacos.client.ui.admin.view.LocationAdminView"
            id="at.rc.tacos.client.view.admin.locationAdminView"
            icon="icons/resources/location.png"
            name="Dienststellen">
      </view>
      
      <view
            allowMultiple="false"
            class="at.rc.tacos.client.ui.admin.view.SickPersonAdminView"
            id="at.rc.tacos.client.view.admin.sickpersonAdminView"
            icon="icons/admin/patient.gif"
            name="Patientendatenbank">
      </view>
      
      <view
            allowMultiple="false"
            class="at.rc.tacos.client.ui.admin.view.StaffMemberAdminView"
            id="at.rc.tacos.client.view.admin.staffMemberAdminView"
            icon="icons/resources/user.png"
            name="Mitarbeiter">
      </view>
      <view
            allowMultiple="false"
            class="at.rc.tacos.client.ui.admin.view.CompetenceAdminView"
            id="at.rc.tacos.client.view.admin.competenceAdminView"
            icon="icons/resources/competence.png"
            name="Kompetenzen">
      </view>
      <view
            allowMultiple="false"
            class="at.rc.tacos.client.ui.admin.view.JobAdminView"
            id="at.rc.tacos.client.view.admin.jobAdminView"
            icon="icons/resources/job.png"
            name="Verwendungen">
      </view>
      <view
            allowMultiple="false"
            class="at.rc.tacos.client.ui.admin.view.AddressAdminView"
            icon="icons/resources/address.png"
            id="at.rc.tacos.client.view.admin.addressAdminView"
            name="Adressen">
      </view>
      <view
            allowMultiple="false"
            class="at.rc.tacos.client.ui.admin.view.DiseaseAdminView"
            icon="icons/resources/disease.png"
            id="at.rc.tacos.client.view.admin.diseaseAdminView"
            name="Erkrankungen">
      </view>
      <view
            allowMultiple="false"
            class="at.rc.tacos.client.ui.view.InfoView"
            id="at.rc.tacos.client.view.info"
            name="Informationen">
      </view>
      <view
            allowMultiple="false"
            class="at.rc.tacos.client.ui.view.ResourceMenuView"
            id="at.rc.tacos.client.view.resourceMenu"
            name="Resource Menü">
      </view>
      <view
            allowMultiple="false"
            class="at.rc.tacos.client.ui.view.TransportMenuView"
            id="at.rc.tacos.client.view.transportMenu"
            name="Transport Menü"
            restorable="true">
      </view>
   </extension>

</plugin>
