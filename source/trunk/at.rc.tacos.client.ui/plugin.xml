<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.ui.splashHandlers">
      <splashHandler
            class="at.rc.tacos.client.ui.splash.LoginSplashHandler"
            id="at.rc.tacos.client.ui.splash">
      </splashHandler>
      <splashHandlerProductBinding
            productId="at.rc.tacos.client.ui.product"
            splashId="at.rc.tacos.client.ui.splash">
      </splashHandlerProductBinding>
   </extension>
   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            application="at.rc.tacos.client.clientApp"
            name="Client Product">
         <property
               name="appName"
               value="Client Product">
         </property>
         <property
               name="aboutText"
               value="Austrian Red Cross.&#x0A;Time and Coordination System - Client Software.&#x0A;Developed by Birgit Thek and Heiß Michael.&#x0A;(c) 2007-2008 FH-Joanneum / ITM">
         </property>
         <property
               name="windowImages"
               value="icons/application/alt_window_16.gif,icons/application/alt_window_32.gif,icons/application/alt_window_48.gif,icons/application/alt_window_64.gif,icons/application/alt_window_128.gif">
         </property>
      </product>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="at.rc.tacos.client.ui.perspectives.ResourcePerspective"
            id="at.rc.tacos.client.ui.perspectives.main"
            name="Resourcen">
      </perspective>
      <perspective
            class="at.rc.tacos.client.ui.perspectives.TransportPerspective"
            id="at.rc.tacos.client.ui.perspectives.transport"
            name="Transporte">
      </perspective>
      <perspective
            class="at.rc.tacos.client.ui.perspectives.TransportPrebookingPerspective"
            id="at.rc.tacos.client.ui.perspectives.transportPrebooking"
            name="Vormerkungen">
      </perspective>
      <perspective
            class="at.rc.tacos.client.ui.perspectives.TransportDialysePerspective"
            id="at.rc.tacos.client.ui.perspectives.transportDialyse"
            name="Dialyse">
      </perspective>
      <perspective
            class="at.rc.tacos.client.ui.perspectives.TransportJournalPerspective"
            id="at.rc.tacos.client.ui.perspectives.transportJournal"
            name="JournalBlatt">
      </perspective>
      <perspective
            class="at.rc.tacos.client.ui.admin.perspectives.AdminPerspective"
            id="at.rc.tacos.client.ui.perspectives.admin"
            name="Administration">
      </perspective>
   </extension>
   
      <extension
         point="org.eclipse.ui.views">
         
      
      <view
            name="Fahrzeugübersicht-Tabelle"
            allowMultiple="false"
            class="at.rc.tacos.client.ui.view.VehiclesViewTable"
            icon="icons/resources/vehicle.png"
            id="at.rc.tacos.client.view.vehiclestable_view">
      </view>
      
      <view
            name="Fahrzeugdetail-Tabelle"
            allowMultiple="false"
            class="at.rc.tacos.client.ui.view.VehiclesViewTableDetailed"
            icon="icons/resources/vehicle.png"
            id="at.rc.tacos.client.view.vehiclestabledetailed_view">
      </view>
      
      <view
            name="Personalübersicht"
            allowMultiple="false"
            class="at.rc.tacos.client.ui.view.PersonalView"
            icon="icons/resources/user.png"
            id="at.rc.tacos.client.view.personal_view">
      </view>
      <view
            allowMultiple="false"
            class="at.rc.tacos.client.ui.view.FilterView"
            icon="icons/resources/vehicle.png"
            id="at.rc.tacos.client.view.filter"
            name="Transport Filter">
      </view>
      <view
            allowMultiple="false"
            class="at.rc.tacos.client.ui.admin.view.VehicleAdminView"
            id="at.rc.tacos.client.view.admin.vehicleAdminView"
            icon="icons/resources/vehicle.png"
            name="Fahrzeuge">
      </view>
      <view
            allowMultiple="false"
            class="at.rc.tacos.client.ui.admin.view.PhoneAdminView"
            id="at.rc.tacos.client.view.admin.phoneAdminView"
            icon="icons/resources/mobilePhone.png"
            name="Mobiltelefone">
      </view>
      <view
            name="Disposition"
            allowMultiple="false"
            class="at.rc.tacos.client.ui.view.UnderwayTransportsView"
            icon="icons/toolbar/viewTransport.png"
            id="at.rc.tacos.client.view.disposition_view">
      </view>
      <view
            name="Offene Transporte"
            allowMultiple="false"
            class="at.rc.tacos.client.ui.view.OutstandingTransportsView"
            icon="icons/toolbar/viewTransport.png"
            id="at.rc.tacos.client.view.outstandingTransports_view">
      </view>
      <view
            name="Journal"
            allowMultiple="false"
            class="at.rc.tacos.client.ui.view.JournalView"
