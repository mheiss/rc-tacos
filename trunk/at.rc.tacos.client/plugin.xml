<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.3"?>
<plugin>
   <extension
         id="application"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run
               class="at.rc.tacos.client.Application">
         </run>
      </application>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">  
      <perspective
            name="Resourcen"
            class="at.rc.tacos.client.perspectives.Perspective"
            id="at.rc.tacos.client.perspectives.client">
      </perspective>
      <perspective
            class="at.rc.tacos.client.perspectives.TransportPerspective"
            id="at.rc.tacos.client.perspectives.transport"
            name="Transporte">
      </perspective>
      <perspective
            class="at.rc.tacos.client.perspectives.TransportPrebookingPerspective"
            id="at.rc.tacos.client.perspectives.transportPrebooking"
            name="Vormerkungen">
      </perspective>
      <perspective
            class="at.rc.tacos.client.perspectives.TransportDialysePerspective"
            id="at.rc.tacos.client.perspectives.transportDialyse"
            name="Dialyse">
      </perspective>
      <perspective
            class="at.rc.tacos.client.perspectives.TransportJournalPerspective"
            id="at.rc.tacos.client.perspectives.transportJournal"
            name="JournalBlatt">
      </perspective>
      <perspective
            class="at.rc.tacos.client.perspectives.AdminPerspective"
            id="at.rc.tacos.client.perspectives.admin"
            name="Administration">
      </perspective>
      <perspective
            class="at.rc.tacos.client.perspectives.LogPerspective"
            id="at.rc.tacos.client.perspectives.log"
            name="LogPerspective">
      </perspective>
   </extension>
   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            application="at.rc.tacos.client.application"
            name="TACOS-Client">
         <property
               name="aboutText"
               value="Austrian Red Cross.&#x0A;Time and Configuration Management">
         </property>
         <property
               name="windowImages"
               value="icons/application/alt_window_16.gif,icons/application/alt_window_32.gif,icons/application/alt_window_48.gif,icons/application/alt_window_64.gif,icons/application/alt_window_128.gif">
         </property>
         <property
               name="appName"
               value="TACOS-Client">
         </property>
         <property
               name="preferenceCustomization"
               value="plugin_customization.ini">
         </property>
      </product>
   </extension>
